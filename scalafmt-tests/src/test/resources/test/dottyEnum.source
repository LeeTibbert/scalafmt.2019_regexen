# 40 columns                            |
runner.dialect = Dotty
maxColumn = 40 # configuration is defined at the top of the file
<<< multi-line
enum Foo {
  case Aaaaaaaaaaaaaaaaaaaa, Bbbbbbbb, C, D, E
}
>>>
enum Foo {
  case
    Aaaaaaaaaaaaaaaaaaaa,
    Bbbbbbbb,
    C,
    D,
    E
}
<<< single-line
enum Foo {
  case Aaaaaaaaaaaaaaaaaaaa, Bbbbbbbb
}
>>>
enum Foo {
  case Aaaaaaaaaaaaaaaaaaaa, Bbbbbbbb
}
<<< comments
enum Foo {
  case Aaaaaaaaaaaaaaaaaaaa,
    // This is b
    Bbbbbbbb
}
>>>
enum Foo {
  case
    Aaaaaaaaaaaaaaaaaaaa,
    // This is b
    Bbbbbbbb
}
<<< ONLY enum
enum Foo {
  case A(a: Int)
  case B(b: Int)
}
>>>
enum Foo {
  case A(a: Int)
  case B(b: Int)
}
